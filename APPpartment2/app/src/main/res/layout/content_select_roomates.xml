<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_select_roomates"
    tools:context="net.brooke.apppartment2.SelectRoomatesActivity">

    <TextView android:text="" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:id="@+id/Person1"
        android:textSize="18dp" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:id="@+id/Person2"
        android:textSize="18dp"
        android:layout_below="@+id/Person1"
        android:layout_alignParentStart="true" />
    <TextView android:text="" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:id="@+id/Person3"
        android:textSize="18dp"
        android:layout_below="@+id/Person2"
        android:layout_alignParentStart="true" />
    <TextView android:text="" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:id="@+id/Person4"
        android:textSize="18dp"
        android:layout_below="@+id/Person3"
        android:layout_alignParentStart="true" />
    <TextView android:text="" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:id="@+id/Person5"
        android:textSize="18dp"
        android:layout_alignTop="@+id/Person4"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp" />
    <TextView android:text="" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:id="@+id/Person6"
        android:textSize="18dp"
        android:layout_below="@+id/Person5"
        android:layout_alignParentStart="true" />
    <TextView android:text="" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:id="@+id/Person7"
        android:textSize="18dp"
        android:layout_below="@+id/Person6"
        android:layout_alignParentStart="true" />
    <TextView android:text="" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:id="@+id/Person8"
        android:textSize="18dp"
        android:layout_below="@+id/Person7"
        android:layout_alignParentStart="true" />
    <TextView android:text="" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:id="@+id/Person9"
        android:textSize="18dp" />
    <TextView android:text="" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:id="@+id/Person10"
        android:textSize="18dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ok"
        android:id="@+id/button8"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="40dp"
        android:onClick="onClickToEven"/>

</RelativeLayout>
